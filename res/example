Family X {
  type R = R1 {n: N}
  Family C {
    type T = T1 {n: N}

    val f: R -> N = lam (x: X). 10
  }
  Family D extends self(X).C {
    type T += T2 {b: B}

    val f: R -> N = lam (x: X). 42
  }
}

Family Y extends X {
  type R += R2 {b: B}

  Family C {
    type T += T3 {t: T}
  }
}